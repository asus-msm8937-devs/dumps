                    parameter="type:mic;tc:1;volume:80;duration:3;file:/persist/FTM_AP/ftm_pcm_record.wav;tc_play:8;"/>
            <module name="TOUCH"
                    parameter="mode:diagonal;"/>
            <module name="FM"
                    parameter="tc:50;volume:50;duration:10;tune:103.7;"/>
        </modules>
        <!-- platform default value for PCBA mode -->
