on property:persist.radio.multisim.config=dsds
    setprop ro.system.simtype 2
    write /data/data/pcbid_status 000001b

on property:persist.radio.multisim.config=dsda
    setprop ro.system.simtype 2
    write /data/data/pcbid_status 000001b

on property:persist.radio.multisim.config=tsts
    setprop ro.system.simtype 2
    write /data/data/pcbid_status 000001b

on property:persist.radio.multisim.config=ss
    setprop ro.system.simtype 1
    write /data/data/pcbid_status 000001a
